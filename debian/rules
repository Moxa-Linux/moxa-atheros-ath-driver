#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_build:
	$(MAKE) -C /lib/modules/4.9.0-11-amd64/build M=$(PWD)/ath modules

override_dh_auto_install:
	$(info Skip dh_auto_install)

override_dh_auto_clean:
	$(MAKE) -C /lib/modules/4.9.0-11-amd64/build M=$(PWD)/ath clean

override_dh_strip:
	dh_strip --no-automatic-dbgsym
